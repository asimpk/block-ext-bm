{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es6",
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "paths": {
      "argon2-browser/dist/argon2.wasm": ["./node_modules/argon2-browser/dist/argon2.wasm"]
    }
  },
  "include": [
    "src"
  ],
  
}
